      <div class="clearfix"></div>
      <div class="col-md-12">
        <div class="tile">
          <h3 class="tile-title">Staff Profile </h3>
          <div class="table-responsive">
            <table class="table">
              <thead>
                <tr>
                  <th>Sl.No</th>
                  <th>First Name</th>                  
                  <th> Department</th>                 
                  <th>Phone</th> 
                  <th>Email</th>
                  <th>Action</th>                 
                  
                </tr>
              </thead>
              <tbody>
                <% for(i=0;i<data.length;i++){%>
                <tr>
                  <td><%=i+1%></td>
                  <td><%=data[i].name%></td>
                  <td><%=data[i].department%></td>
                  <td><%=data[i].phone%></td>
                  <td><%=data[i].email%></td>
                  <td> 
                    <% if(data[i].status === "1"){%>
                      <a class="btn btn-outline-danger" style="width: 85px;" href="/profile/delete-staff/<%=data[i]._id%>" type="button">Delete</a>
                  <%}else{%>
                    <a class="btn btn-outline-primary" href="/profile/approve-staff/<%=data[i]._id%>" type="button">Approve</a> 
                    <a class="btn btn-outline-danger" href="/profile/delete-staff/<%=data[i]._id%>" style="width: 85px;" type="button">Reject</a>
                  
                    <%}%>
                            
                  </td>

                </tr>
                
                <%}%> 
              </tbody>
            </table>
          </div>
        </div>
      </div>
      </div>
